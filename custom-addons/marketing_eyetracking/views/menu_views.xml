<odoo>
    <!-- Acción para Analyses -->
    <record id="action_eyetracking_analysis" model="ir.actions.act_window">
        <field name="name">Eye Tracking Analyses</field>
        <field name="res_model">eyetracking.analysis</field>
        <field name="view_mode">tree,form</field>
    </record>

    <!-- Acción para Sessions -->
    <record id="action_eyetracking_session" model="ir.actions.act_window">
        <field name="name">Eye Tracking Sessions</field>
        <field name="res_model">eyetracking.session</field>
        <field name="view_mode">tree,form</field>
    </record>

    <!-- Menú principal -->
    <menuitem id="menu_eyetracking_root" name="Eye Tracking" sequence="1"/>

    <!-- Submenús -->
    <menuitem id="menu_eyetracking_analysis" name="Analyses" parent="menu_eyetracking_root" sequence="2" action="action_eyetracking_analysis"/>
    <menuitem id="menu_eyetracking_session" name="Sessions" parent="menu_eyetracking_root" sequence="3" action="action_eyetracking_session"/>
</odoo>
