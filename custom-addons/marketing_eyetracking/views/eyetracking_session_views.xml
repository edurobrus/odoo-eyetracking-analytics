<odoo>
    <record id="view_eyetracking_session_tree" model="ir.ui.view">
        <field name="name">eyetracking.session.tree</field>
        <field name="model">eyetracking.session</field>
        <field name="arch" type="xml">
            <tree string="Eye Tracking Sessions">
                <field name="name"/>
                <field name="analysis_id"/>
                <field name="duration"/>
                <field name="participant_count"/>
            </tree>
        </field>
    </record>

    <record id="view_eyetracking_session_form" model="ir.ui.view">
        <field name="name">eyetracking.session.form</field>
        <field name="model">eyetracking.session</field>
        <field name="arch" type="xml">
            <form string="Eye Tracking Session">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="analysis_id"/>
                        <field name="duration"/>
                        <field name="participant_count"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
